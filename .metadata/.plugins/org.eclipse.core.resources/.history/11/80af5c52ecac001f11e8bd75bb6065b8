package com.project.managerEmployees.Entity;

import java.util.Date;

import jakarta.persistence.Column;
import jakarta.persistence.Entity;
import jakarta.persistence.GeneratedValue;
import jakarta.persistence.GenerationType;
import jakarta.persistence.Id;
import jakarta.persistence.Table;
@Entity
@Table(name = "ManagerEmployee")
public class Project {

    @Id
    @GeneratedValue(strategy = GenerationType.IDENTITY)
    private Integer id;

    @Column(name = "ProjectName")
    private String projectName;
    
    @Column(name ="Client")
    private String client;

    @Column(name = "TypeOfProject")
    private String typeOfProject;

    @Column(name = "SubmissionMethod")
    private String submissionMethod;

    @Column(name = "Status")
    private String status;

    @Column(name = "StartDatePlanned")
    private Date startDatePlanned;

    @Column(name = "StartDateActual")
    private Date startDateActual;

    @Column(name = "EndDatePlanned")
    private Date endDatePlanned;

    @Column(name = "EndDateActual")
    private Date endDateActual;
	public Project() {
		
	}

	public Project(Long id, String projectName, String client, String typeOfProject, String submissionMethod,
			String status, Date startDatePlanned, Date startDateActual, Date endDatePlanned, Date endDateActual) {
		super();
		this.id = id;
		this.projectName = projectName;
		this.client = client;
		this.typeOfProject = typeOfProject;
		this.submissionMethod = submissionMethod;
		this.status = status;
		this.startDatePlanned = startDatePlanned;
		this.startDateActual = startDateActual;
		this.endDatePlanned = endDatePlanned;
		this.endDateActual = endDateActual;
	}

	public Project(String projectName, String client, String typeOfProject, String submissionMethod, String status,
			Date startDatePlanned, Date startDateActual, Date endDatePlanned, Date endDateActual) {
		super();
		this.projectName = projectName;
		this.client = client;
		this.typeOfProject = typeOfProject;
		this.submissionMethod = submissionMethod;
		this.status = status;
		this.startDatePlanned = startDatePlanned;
		this.startDateActual = startDateActual;
		this.endDatePlanned = endDatePlanned;
		this.endDateActual = endDateActual;
	}

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public String getProjectName() {
		return projectName;
	}

	public void setProjectName(String projectName) {
		this.projectName = projectName;
	}

	public String getClient() {
		return client;
	}

	public void setClient(String client) {
		this.client = client;
	}

	public String getTypeOfProject() {
		return typeOfProject;
	}

	public void setTypeOfProject(String typeOfProject) {
		this.typeOfProject = typeOfProject;
	}

	public String getSubmissionMethod() {
		return submissionMethod;
	}

	public void setSubmissionMethod(String submissionMethod) {
		this.submissionMethod = submissionMethod;
	}

	public String getStatus() {
		return status;
	}

	public void setStatus(String status) {
		this.status = status;
	}

	public Date getStartDatePlanned() {
		return startDatePlanned;
	}

	public void setStartDatePlanned(Date startDatePlanned) {
		this.startDatePlanned = startDatePlanned;
	}

	public Date getStartDateActual() {
		return startDateActual;
	}

	public void setStartDateActual(Date startDateActual) {
		this.startDateActual = startDateActual;
	}

	public Date getEndDatePlanned() {
		return endDatePlanned;
	}

	public void setEndDatePlanned(Date endDatePlanned) {
		this.endDatePlanned = endDatePlanned;
	}

	public Date getEndDateActual() {
		return endDateActual;
	}

	public void setEndDateActual(Date endDateActual) {
		this.endDateActual = endDateActual;
	}

	@Override
	public String toString() {
		return "Project [id=" + id + ", projectName=" + projectName + ", client=" + client + ", typeOfProject="
				+ typeOfProject + ", submissionMethod=" + submissionMethod + ", status=" + status
				+ ", startDatePlanned=" + startDatePlanned + ", startDateActual=" + startDateActual
				+ ", endDatePlanned=" + endDatePlanned + ", endDateActual=" + endDateActual + "]";
	}
	
	
	
}